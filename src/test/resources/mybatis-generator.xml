<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="mysqlTable" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>

        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
            <property name="searchString" value="Example$"/>
            <property name="replaceString" value="Criteria"/>
        </plugin>

        <commentGenerator type="com.demo.ums.generator.CustomCommentGenerator">
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/ums?characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=Asia/Shanghai&amp;allowPublicKeyRetrieval=true"
                        userId="root"
                        password="123456">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>


        <javaModelGenerator targetPackage="com.demo.ums.repository.model" targetProject="src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="com.demo.ums.repository.mapper" targetProject="src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="user" domainObjectName="UserDO" mapperName="UserMapper" sqlProviderName="UserSqlProvider" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
            <generatedKey column="userId" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="role" domainObjectName="RoleDO" mapperName="RoleMapper" sqlProviderName="RoleSqlProvider" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
            <generatedKey column="roleId" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="permission_group" domainObjectName="PermissionGroupDO" mapperName="PermissionGroupMapper" sqlProviderName="PermissionGroupSqlProvider"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
            <generatedKey column="permissionGroupId" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="permission" domainObjectName="PermissionDO" mapperName="PermissionMapper" sqlProviderName="PermissionSqlProvider" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
            <generatedKey column="permissionId" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="role_permission" domainObjectName="RolePermissionDO" mapperName="RolePermissionMapper" sqlProviderName="RolePermissionSqlProvider"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
        <table tableName="user_role" domainObjectName="UserRoleDO" mapperName="UserRoleMapper" sqlProviderName="UserRoleSqlProvider" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" enableCountByExample="false">
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>
</generatorConfiguration>